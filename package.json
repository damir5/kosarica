{
  "name": "kosarica",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:test": "BUILD_ENV=test vite build --mode test",
    "build:prod": "BUILD_ENV=prod vite build --mode prod",
    "preview": "pnpm build && wrangler dev --config wrangler.jsonc",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "release:test": "rm -rf dist .wrangler/deploy && pnpm build:test && wrangler deploy --config wrangler.test.jsonc",
    "release:prod": "rm -rf dist .wrangler/deploy && pnpm build:prod && wrangler deploy --config wrangler.prod.jsonc",
    "cf:generate": "wrangler types --env-interface Env",
    "generate": "pnpm cf:generate && pnpm db:generate",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply kosarica-db --local",
    "db:migrate:test": "wrangler d1 migrations apply kosarica-db-test --config wrangler.test.jsonc --remote",
    "db:migrate:prod": "wrangler d1 migrations apply kosarica-db-prod --config wrangler.prod.jsonc --remote",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@better-auth/passkey": "^1.4.9",
    "@cloudflare/vite-plugin": "^1.13.8",
    "@faker-js/faker": "^10.0.0",
    "@orpc/client": "^1.7.5",
    "@orpc/json-schema": "^1.7.5",
    "@orpc/openapi": "^1.7.5",
    "@orpc/server": "^1.7.5",
    "@orpc/tanstack-query": "^1.7.5",
    "@orpc/zod": "^1.7.5",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/react-table": "^8.21.2",
    "@tanstack/router-plugin": "^1.132.0",
    "better-auth": "^1.4.9",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.0",
    "lucide-react": "^0.561.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.3.6",
    "vite-tsconfig-paths": "^6.0.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@cloudflare/workers-types": "^4.20251225.0",
    "@tanstack/devtools-vite": "^0.3.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv": "^16.0.0",
    "jsdom": "^27.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.56.0"
  },
  "sideEffects": false,
  "type": "module"
}
