{
  "name": "kosarica",
  "private": true,
  "scripts": {
    "setup-dev": "tsx scripts/setup-dev.ts",
    "dev": "vite dev",
    "build": "vite build",
    "build:test": "BUILD_ENV=test vite build --mode test",
    "build:prod": "BUILD_ENV=prod vite build --mode prod",
    "preview": "pnpm build && wrangler dev --config wrangler.jsonc",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "release:test": "rm -rf dist .wrangler/deploy && pnpm build:test && wrangler deploy --config dist/server/wrangler.json",
    "release:prod": "rm -rf dist .wrangler/deploy && pnpm build:prod && wrangler deploy --config dist/server/wrangler.json",
    "cf:generate": "wrangler types --env-interface Env",
    "generate": "pnpm cf:generate && pnpm db:generate",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply kosarica-db --local",
    "db:migrate:test": "wrangler d1 migrations apply kosarica-db-test --config wrangler.test.jsonc --remote",
    "db:migrate:prod": "wrangler d1 migrations apply kosarica-db-prod --config wrangler.prod.jsonc --remote",
    "db:studio": "drizzle-kit studio",
    "db:sync-test": "tsx scripts/sync-db.ts --source=test",
    "db:sync-prod": "tsx scripts/sync-db.ts --source=prod",
    "analyze:price-variation": "node scripts/analyze-price-variation.mjs",
    "ingest": "tsx src/ingestion/cli/index.ts"
  },
  "dependencies": {
    "@better-auth/passkey": "^1.4.13",
    "@cloudflare/vite-plugin": "^1.21.0",
    "@faker-js/faker": "^10.2.0",
    "@orpc/client": "^1.13.4",
    "@orpc/json-schema": "^1.13.4",
    "@orpc/openapi": "^1.13.4",
    "@orpc/server": "^1.13.4",
    "@orpc/tanstack-query": "^1.13.4",
    "@orpc/zod": "^1.13.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-query": "^5.90.17",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.150.0",
    "@tanstack/react-router-devtools": "^1.150.0",
    "@tanstack/react-router-ssr-query": "^1.150.0",
    "@tanstack/react-start": "^1.150.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-plugin": "^1.150.0",
    "@types/leaflet": "^1.9.21",
    "adm-zip": "^0.5.16",
    "better-auth": "^1.4.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "fast-xml-parser": "^5.3.3",
    "fflate": "^0.8.2",
    "iconv-lite": "^0.7.2",
